{
  "name": "@qpd-v/mcp-DEEPwebresearch",
  "version": "0.2.0",
  "description": "Enhanced MCP server for deep web research",
  "license": "MIT",
  "author": "qpd-v",
  "homepage": "https://github.com/qpd-v/mcp-DEEPwebresearch",
